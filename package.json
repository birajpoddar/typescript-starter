{
  "name": "typescript-starter",
  "version": "1.0.0",
  "description": "Start of Node Journey with TS and jasmine",
  "main": "build/index.js",
  "scripts": {
    "build": "npx tsc",
    "start": "npm run build && node build/.",
    "lint": "npx eslint src/**",
    "prettier": "prettier src/** --write",
    "jasmine": "jasmine",
    "test": "npm run build && npm run jasmine",
    "start-dev": "nodemon src/api/index.ts"
  },
  "keywords": [
    "node",
    "start"
  ],
  "author": "Biraj Poddar",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jasmine": "^4.0.3",
    "@types/node": "^18.0.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.30.5",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "nodemon": "^2.0.19",
    "prettier": "^2.7.1",
    "ts-node": "^10.8.2",
    "typescript": "^4.7.4"
  },
  "dependencies": {
    "express": "^4.18.1",
    "jasmine": "^4.2.1",
    "jasmine-spec-reporter": "^7.0.0",
    "supertest": "^6.2.4"
  }
}
